{
  "manifest_version": 2,
  "version": "1.0.0",

  "name": "BabyTab",
  "description": "A simple hello world!",
  "background": {
    "scripts": ["src/js/background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "src/html/index.html"
  },
  "permissions": [
    "tabs"
  ],

  "icons": {
    "128": "img/icon128.png",
    "48": "img/icon48.png",
    "16": "img/icon16.png"
  }
}