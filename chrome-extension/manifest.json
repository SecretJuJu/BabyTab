{
  "manifest_version": 2,
  "version": "1.0.0",

  "name": "BabyTab",
  "description": "A simple hello world!",
  "background": {
    "scripts": ["src/js/background.js"],
    "persistent": false
  },
  "options_page": "src/html/detail.html",
  "browser_action": {
    "default_popup": "src/html/popUp.html"
  },
  "permissions": [
    "storage",
    "tabs"
  ],

  "icons": {
    "128": "icons/icon128.png",
    "48": "icons/icon48.png",
    "16": "icons/icon16.png"
  }
}